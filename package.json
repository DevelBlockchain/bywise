{
  "name": "bywise",
  "version": "2.1.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_OPTIONS=--max-old-space-size=512 nodemon --watch 'src/' --exec 'ts-node src/index.ts -log -chain ./local.json -start-debug -port 3001' -e ts",
    "start:1": "nodemon --watch 'src/' --exec 'ts-node src/index.ts -log -name node1 -reset -chain ./local.json -start-debug -port 3001' -e ts",
    "start:2": "nodemon --watch 'src/' --exec 'ts-node src/index.ts -log -name node2 -reset -chain ./local.json -new-wallet -start-debug -port 3005 -pnodes 3001' -e ts",
    "start:node1": "ts-node src/index.ts -log -reset -new-chain-local local -delay-block 30 -name node1 -chain ./local.json -start-debug -port 3001",
    "start:node2": "ts-node src/index.ts -log -reset -name node2 -chain ./local.json -start-debug -new-wallet -port 3005 -pnodes 3001",
    "start:test": "nodemon --watch 'src/' --exec 'ts-node src/test.ts -debug' -e ts",
    "start:reset": "nodemon --watch 'src/' --exec 'ts-node src/index.ts -all -debug -reset' -e ts",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --watch true --maxWorkers=1 --config jestconfig.json",
    "test:all": "BYWISE_TEST=1 jest --verbose true --maxWorkers=5 --config jestconfig.json",
    "test:single": "npm run test:clear && npm test -- blocks-propagation.test.ts",
    "test:clear": "jest --clearCache"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bywise/web3": "^0.9.99",
    "abort-controller": "^3.0.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "gh-pages": "^5.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "level": "^8.0.0",
    "path": "^0.12.7",
    "pidusage": "^3.0.2",
    "quickjs-emscripten": "^0.21.1",
    "randomstring": "^1.2.2",
    "seedrandom": "^3.0.5",
    "sha256": "^0.2.0",
    "web3": "^1.8.0",
    "web3-eth-contract": "^1.8.0",
    "web3-utils": "^1.8.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.3",
    "@types/amqplib": "^0.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.11.26",
    "@types/randomstring": "^1.1.8",
    "@types/sha256": "^0.2.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "jest": "^27.5.1",
    "nodemon": "^2.0.20",
    "pkg": "^5.8.0",
    "pm2": "^5.2.0",
    "prettier": "^2.6.0",
    "supertest": "^6.2.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  }
}
