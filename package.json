{
  "name": "bywise",
  "version": "2.1.7",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_OPTIONS=--max-old-space-size=512 nodemon --watch 'src/' --exec 'ts-node src/index.ts -log -chain ./local.json -start-debug -port 3001' -e ts",
    "start:1": "nodemon --watch 'src/' --exec 'ts-node src/index.ts -log -name node1 -reset -chain ./local.json -start-debug -port 3001' -e ts",
    "start:2": "nodemon --watch 'src/' --exec 'ts-node src/index.ts -log -name node2 -reset -chain ./local.json -new-wallet -start-debug -port 3005 -pnodes 3001' -e ts",
    "build": "tsc && pkg -c pkg.json ./dist/index.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --watch true --maxWorkers=1 --config jestconfig.json",
    "test:all": "BYWISE_TEST=1 jest --verbose true --maxWorkers=5 --config jestconfig.json",
    "test:single": "npm run test:clear && npm test -- transactions.test.ts",
    "test:clear": "jest --clearCache"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bywise/web3": "^1.0.1",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^9.0.0",
    "level": "^8.0.0",
    "path": "^0.12.7",
    "pidusage": "^3.0.2",
    "quickjs-emscripten": "^0.21.1",
    "randomstring": "^1.2.2",
    "seedrandom": "^3.0.5",
    "sha256": "^0.2.0",
    "web3": "^1.8.0",
    "web3-eth-contract": "^1.8.0",
    "web3-utils": "^1.8.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.8",
    "@types/randomstring": "^1.1.8",
    "@types/sha256": "^0.2.0",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "pkg": "^5.8.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.4.5"
  }
}